 {
  "expo": {
    "name": "NIMSHMIS",
    "slug": "NIMSHMIS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cdac.nimshmis",
      "buildNumber": "3.0.0",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Please allow Location.",
        "NSPhotoLibraryUsageDescription": "Relax, your information is in good hands."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cdac.nimshmis",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
